journal = {PLoS ONE},
month = sep,
number = {9},
pages = {e12704},
publisher = {Public Library of Science},
title = {{\{S\}ystematic \{C\}haracterizations of \{T\}ext \{S\}imilarity in \{F\}ull \{T\}ext \{B\}iomedical \{P\}ublications}},
volume = {5},
