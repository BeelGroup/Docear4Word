keywords = { Average Precision, Bpref, Incomplete Judgments, Sampling,Evaluation},
month = nov,
publisher = {ACM},
title = {{\{E\}stimating \{A\}verage \{P\}recision with \{I\}ncomplete and \{I\}mperfect \{J\}udgments}},
year = {2006}
