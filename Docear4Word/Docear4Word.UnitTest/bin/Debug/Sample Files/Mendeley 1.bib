@article{ChristensenHughes06,
abstract = {Research suggests that the majority of U.S. undergraduate students
have engaged in some form of misconduct while completing their academic
work, despite knowing that such behaviour is ethically or morally
wrong. U.S.-based studies have also identified myriad personal and
institutional factors associated with academic misconduct. Implicit
in some of these factors are several institutional strategies that
