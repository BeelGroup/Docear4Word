@techreport{Sorokina06,
abstract = {We describe a large-scale application of methods for finding plagiarism
in research document collections. The methods are applied to a collection
of 284,834 documents collected by arXiv.org over a 14 year period,
covering a few different research disciplines. The methodology efficiently
