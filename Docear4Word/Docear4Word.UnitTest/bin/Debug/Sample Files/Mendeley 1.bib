pages = {215--228},
publisher = {Springer Berlin Heidelberg},
series = {Lecture Notes in Computer Science},
title = {{\{U\}sing \{W\}ord \{C\}lusters to \{D\}etect \{S\}imilar \{W\}eb \{D\}ocuments}},
volume = {4092},
year = {2006}
}
