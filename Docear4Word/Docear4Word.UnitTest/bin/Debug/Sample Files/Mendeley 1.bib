keywords = {Levenshtein distance;Smith-Waterman algorithm;acad},
pages = {569},
title = {{\{P\}lagiarism \{D\}etection \{U\}sing the \{L\}evenshtein \{D\}istance and \{S\}mith-\{W\}aterman \{A\}lgorithm}},
