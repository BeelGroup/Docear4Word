@article{Koppel10,
abstract = {Most previous work on authorship attribution has focused on the case
in which we need to attribute an anonymous document to one of a small
set of candidate authors. In this paper, we consider authorship attribution
as found in the wild: the set of known candidates is extremely large
(possibly many thousands) and might not even include the actual author.
Moreover, the known texts and the anonymous texts might be of limited
length. We show that even in these difficult cases, we can use similarity-based
methods along with multiple randomized feature sets to achieve high
precision. Moreover, we show the precise relationship between attribution
precision and four parameters: the size of the candidate set, the
quantity of known-text by the candidates, the length of the anonymous
text and a certain robustness score associated with a attribution.},
author = {Koppel, Moshe and Schler, Jonathan and Argamon, Shlomo},
doi = {10.1007s10579-009-9111-2},
file = {:C$\backslash$:/dissertation/literature/Koppel, Schler, Argamon/Language Resources and Evaluation/Koppel, Schler, Argamon - 2010 - \{A\}uthorship attribution in the wild.bib:bib},
issn = {1574-020X},
journal = {Language Resources and Evaluation},
pages = {1--12},
publisher = {Springer Netherlands},
title = {{\{A\}uthorship attribution in the wild}},
volume = {[Online Re},
year = {2010}
}
